<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>H1</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.hfut.LoginServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>AddServlet</servlet-name>
    <servlet-class>com.hfut.AddServlet</servlet-class>
  </servlet>
  
    <servlet>
    <servlet-name>DeleteServlet</servlet-name>
    <servlet-class>com.hfut.DeleteServlet</servlet-class>
  </servlet>
  
    <servlet>
    <servlet-name>SendLogServlet</servlet-name>
    <servlet-class>com.hfut.SendLogServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  
    <servlet-mapping>
    <servlet-name>AddServlet</servlet-name>
    <url-pattern>/Add</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
    <servlet-name>DeleteServlet</servlet-name>
    <url-pattern>/Delete</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
    <servlet-name>SendLogServlet</servlet-name>
    <url-pattern>/SendLog</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
</web-app>